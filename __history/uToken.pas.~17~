unit uToken;

interface

uses
  System.Classes;

type
  TToken = class
  private
    { Private declarations }
    FTokenList: TStringList;
  public
    { Public declarations }
    constructor Create;
    destructor Destroy; override;
    function Display: TStringList;
    function Authenticate(Token: string): Boolean;
    procedure Add(Value: string);
  end;

implementation


end.
